<%= form_with(model: @article, local: true) do |form| %> 
<!-- local true para que no mande por ajax -->
  <div class="field">
    <%=form.label :title %>
    <%=form.text_field :title %>
  </div>
  <div class="field">
    <%=form.label :content %>
    <%=form.rich_text_area :content %>
  </div>
  <div>
    <%=form.submit %>
  </div>
<%end%>